  <body>
    <div class="navbar navbar-fixed-top navbar-inverse">
      <div class="navbar-inner">
        <div class="container">
          <a class="brand" href="/"> GRUBBY </a>
          <ul class="nav">
            <li>
              <a href="#" target="grubbyhelp.html"> Help </a>
            </li>
            <li>
              <a href="#" target="grubbyabout.html"> About </a>
            </li>
          </ul>
        </div>
      </div>
    </div>
    <div class="container">
      <form action='deploy' method='post'>
        <div class="control-group">
          <label for="grubbytag"> <h3> Tag </h3> </label>
          <select name="grubbytag">
            <optgroup label="Tags">
              <% @tags.each do |tag,ct| %>
                <% if ct == @nodec %>
                  <%= "<option value=\"#{tag}\">#{tag}</option>" %>
                <% else %>
                  <%= "<option value=\"#{tag}\" disabled>#{tag}</option>" %>
                <% end %>
            <% end %>
            <optgroup label="Branches">
              <% @branches.each do |branch,ct| %>
                <% if ct == @nodec %>
                  <%= "<option value=\"#{branch}\">#{branch}</option>" %>
                <% else %>
                  <%= "<option value=\"#{branch}\" disabled>#{branch}</option>" %>
                <% end %>
            <% end %>
          </select>
          <!-- 
          <label class="checkbox">
            <input type="checkbox"><h3> Feature branch</h3>
          </label>
          -->
        </div>
        <button class="btn btn-danger btn-large btn-primary" type="submit">ohgodidon'tcarejustmakeitgo</button>
      </form>
      <div class="control-group">
      </div>
    </div>

    <div class="container">
    <h3>Current state</h3>
      <%= "<pre>#{@frod}</pre>" %>
    </div>


